{
  "name": "kit",
  "workspaces": [
    "packages/js",
    "packages/react",
    "packages/node",
    "packages/providers",
    "packages/shared",
    "packages/server",
    "packages/worker"
  ],
  "scripts": {
    "install:all": "npm i && cd docs && npm i",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "database:generate": "npm run database:generate -w @kit/shared",
    "database:migrate": "npm run database:migrate -w @kit/shared",
    "clean:all": "npm run clean --workspaces",
    "build:all": "npm run database:generate && npm run build --workspaces",
    "test:all": "npm run test --workspaces",
    "server:dev": "npm run dev -w @kit/server",
    "worker:dev": "npm run dev -w @kit/worker",
    "docs:dev": "cd docs && npm run dev"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "prettier-plugin-organize-imports": "^3.2.4"
  },
  "dependencies": {
    "@temporalio/client": "^1.9.1"
  }
}
